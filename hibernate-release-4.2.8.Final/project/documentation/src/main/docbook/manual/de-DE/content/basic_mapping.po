"\n"
"@Entity \n"
"class User {\n"
"   @EmbeddedId UserId id;\n"
"   Integer age;\n"
"}\n"
"\n"
"@Embeddable\n"
"class UserId implements Serializable {\n"
"   String firstName;\n"
"   String lastName;\n"
"\n"
"   //implements equals and hashCode\n"
"}"
msgstr ""

#. Tag: para
#: basic_mapping.xml:706
#, no-c-format
msgid ""
"In this case <classname>Customer</classname> is its own identifier "
