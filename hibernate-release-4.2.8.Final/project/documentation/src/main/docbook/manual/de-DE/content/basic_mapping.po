#, no-c-format
msgid ""
"@Lob\n"
"public String getFullText() {\n"
"    return fullText;\n"
"}\n"
"\n"
"@Lob\n"
"public byte[] getFullCode() {\n"
"    return fullCode;\n"
"}"
msgstr ""

#. Tag: para
#: basic_mapping.xml:1772
