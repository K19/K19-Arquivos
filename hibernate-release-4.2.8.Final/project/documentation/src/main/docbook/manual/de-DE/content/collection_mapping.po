"\n"
"   @OneToMany(mappedBy=\"customer\")\n"
"   @OrderBy(\"number\")\n"
"   public List&lt;Order&gt; getOrders() { return orders; }\n"
"   public void setOrders(List&lt;Order&gt; orders) { this.orders = "
"orders; }\n"
"   private List&lt;Order&gt; orders;\n"
"}\n"
"\n"
